						</ExistIf>
					</Listener>
				</BlockEvent>
			</Bunch>
			<Bunch prob="15" >
				<BlockEvent block="Classic:T" initPos="+4" placingMillisec="1000">
					<Listener groupName="COULD_PLACE" event="EVID-VAR-BLCK-DURATION2" msgName="RESET"/>
					<Listener groupName="FREEZED" event="EVID-SND-FREEZED" msgName="PLAY_FREE">
						<ExistIf>
